.popUpMensagemStatus h2.msgErro { background: url("../img/icone-mensagem-erro.gif") no-repeat scroll left center transparent; font-size: 12px; height: 29px; margin: 0 auto; padding-left: 40px; padding-top: 13px; text-align: center; width: -moz-fit-content; color:red;  }
.popUpMensagemStatus input.fecharPopUpMsg { left: -5px; margin-bottom: 13%; margin-left: 50%; position: relative; padding-top: 8%}

.overlayDiv { background-image: url(../img/bg_loading.png); position: fixed; left: 0px; top: 0px; right: 0px; bottom: 0px; height: 100%; width: 100%; z-index: 2; visibility: hidden; }
.overlayDiv .mensagem {  background-color: #FFFFFF; border: 1px solid #CCCCCC; border-radius: 5px 5px 5px 5px; left: 50%; margin-left: -75px; margin-top: -22px; padding: 10px; position: fixed; top: 50%; }

/*Div organizadora de conteudo*/
.organiza_input { float:left; margin: 10px 5px 10px 0; }
.organiza_input label { padding: 5px 20px 5px 0; display:block; }
.organiza_input_container { padding: 5px 5px 10px 0; overflow:hidden; clear:both; }


.exibeComponentes {
	display: block;
}

.escondeComponentes {
	display: none;
}

.popup{
	display: inline-block; position: fixed; top: 130px; right: 40px; border: 1px solid black; background-color: #001A33; width: 350px; height: 25px;
}
.popupBarraAzul{
	font-size:14px; color: white; margin: 0; position: absolute; top: 45%; left: 50%; margin-right: -50%; transform: translate(-50%, -50%);
}
.popupBtnFechar{
	font-size:14px; color: white; margin: 0; position: absolute; top: 45%; left: 95%; margin-right: -50%; transform: translate(-50%, -50%);
}
.popupConteudo{
	display: inline-block; position: fixed; top: 155px; right: 40px; border: 1px solid black; background-color: #ffffff; width: 350px; height: 60px;
}
.popupTextoConteudo{
	font-size:14px; color: red; margin: 0; position: absolute; top: 45%; left: 50%; margin-right: -50%; transform: translate(-50%, -50%);
}
