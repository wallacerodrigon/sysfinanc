package br.net.walltec.api.excecoes;

/**
 * @author wallace
 *
 */
public class ConversorNaoExistenteException extends NegocioException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public ConversorNaoExistenteException(String message) {
		super(message);
	}

	public ConversorNaoExistenteException(String message, Throwable cause) {
		super(message, cause);
	}

	public ConversorNaoExistenteException(Throwable cause) {
		super(cause);
	}
}
