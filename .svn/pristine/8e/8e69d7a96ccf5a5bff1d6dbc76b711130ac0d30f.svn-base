import { Component } from '@angular/core';
import {LoginVO} from './dominio/vo/login-vo';

@Component({
  selector: 'app-root',
  templateUrl: './app.component.html',
  styleUrls: ['./app.component.css']
})
export class AppComponent {
  title = 'Gyman';
  nomeUsuario: string;
  descVersaoSistema: string = "";

  constructor(){
    let usuario: string = sessionStorage.getItem("usuario");
    if (usuario){
       let usr: LoginVO = JSON.parse(usuario);
       this.nomeUsuario = usr.nomeUsuario.substr(0,15);
       this.descVersaoSistema = usr.versaoAtual.versao + " - " + usr.versaoAtual.dataHoraUltimaVersao;
    }    
  }
}
