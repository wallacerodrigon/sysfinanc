import { NgModule } from '@angular/core';
import { LotacoesEditarComponent } from './lotacoes-editar/lotacoes-editar.component';
import { LotacoesIncluirComponent } from './lotacoes-incluir/lotacoes-incluir.component';
import { AuthGuard } from '../../app.auth-guard';
import { LotacoesListComponent } from './lotacoes-list/lotacoes-list.component';
import { LotacoesComponent } from './lotacoes.component';
import { RouterModule, Routes } from '@angular/router';

const lotacoesRoutes: Routes = [
  {
    path: '',
    component: LotacoesComponent,
    children: [
      {
        path: '',
        component: LotacoesListComponent,
        children: [
          {
            path: 'incluir',
            component: LotacoesIncluirComponent
          },
          {
            path: 'editar/:id',
            component: LotacoesEditarComponent,
          }
        ]
      }
    ]
  }
];

@NgModule({
  imports: [
    RouterModule.forChild(lotacoesRoutes)
  ],
  exports: [
    RouterModule
  ]
})
export class LotacoesRoutingModule {}
