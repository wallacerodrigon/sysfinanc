package br.net.walltec.api.vo;

/**
 * @author wallace
 *
 */
public class PlanoPagamentoVO extends GerenciadorPadraoVO {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private Integer idPlanoPagamento;

	private String descPlanoPagamento;

	private String bolParcelamento;

	private Integer qtdParcelas;

	/**
	 * @return the idPlanoPagamento
	 */
	public Integer getIdPlanoPagamento() {
		return idPlanoPagamento;
	}

	/**
	 * @param idPlanoPagamento
	 *            the idPlanoPagamento to set
	 */
	public void setIdPlanoPagamento(Integer idPlanoPagamento) {
		this.idPlanoPagamento = idPlanoPagamento;
	}

	/**
	 * @return the descPlanoPagamento
	 */
	public String getDescPlanoPagamento() {
		return descPlanoPagamento;
	}

	/**
	 * @param descPlanoPagamento
	 *            the descPlanoPagamento to set
	 */
	public void setDescPlanoPagamento(String descPlanoPagamento) {
		this.descPlanoPagamento = descPlanoPagamento;
	}

	/**
	 * @return the bolParcelamento
	 */
	public String getBolParcelamento() {
		return bolParcelamento;
	}

	/**
	 * @param bolParcelamento
	 *            the bolParcelamento to set
	 */
	public void setBolParcelamento(String bolParcelamento) {
		this.bolParcelamento = bolParcelamento;
	}

	/**
	 * @return the qtdParcelas
	 */
	public Integer getQtdParcelas() {
		return qtdParcelas;
	}

	/**
	 * @param qtdParcelas
	 *            the qtdParcelas to set
	 */
	public void setQtdParcelas(Integer qtdParcelas) {
		this.qtdParcelas = qtdParcelas;
	}

}
